<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://code.alibabatech.com/schema/dubbo
           http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:annotation package="com.d2c.main.openapi" />
	<import resource="classpath*:/META-INF/spring-consumer/consumer-base.xml" />
	<import resource="classpath*:/META-INF/spring-consumer/consumer-order.xml" />
	<import resource="classpath*:/META-INF/spring-consumer/consumer-product.xml" />
</beans>